        "sass-loader": "^7.1.0",
        "ts-jest": "^23.0.0",
        "typescript": "^3.4.3",
        "vue-template-compiler": "^2.5.21"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "jest": {
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "vue",
            "ts",
            "tsx"
        ],
        "transform": {
            "^.+\\.vue$": "vue-jest",
            ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
            "^.+\\.tsx?$": "ts-jest"
        },
        "transformIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "jest-serializer-vue"
        ],
        "testMatch": [
            "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
        ],
        "testURL": "http://localhost/",
        "watchPlugins": [
            "/Users/viking/projects/personal/FormidableForms/packages/vue-components/vue-components/@formidable/vue-components/node_modules/jest-watch-typeahead/filename.js",
            "/Users/viking/projects/personal/FormidableForms/packages/vue-components/vue-components/@formidable/vue-components/node_modules/jest-watch-typeahead/testname.js"
        ],
        "globals": {
            "ts-jest": {
                "babelConfig": true
            }
        }
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.ts": [
            "vue-cli-service lint",
            "git add"
        ],
        "*.vue": [
            "vue-cli-service lint",
            "git add"
        ]
    },
    "gitHead": "bca8d33e8e926e2a29385d8b355d6e9628791bf3"
